{
  "name": "bastman/php7-arrayly",
  "description": "a simple port of Kotlin Collections to php",
  "keywords": [
    "array", "manipulation", "utility",
    "helpers", "utils"
  ],
  "homepage": "https://github.com/bastman/php7-Arrayly",
  "license": "MIT",
  "authors": [
    {
      "name": "Seb Schmidt"
    }
  ],
  "require": {
    "php": ">=7.1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.1"
  },
  "support": {
    "issues": "https://github.com/bastman/php7-Arrayly",
    "source": "https://github.com/bastman/php7-Arrayly"
  },
  "autoload": {
    "psr-4": { "Arrayly\\": "src/" },
    "files": [
      "src/functions.php",
      "src/fn/array.fn.base.php",
      "src/fn/array.fn.map.php",
      "src/fn/array.fn.take.php",
      "src/fn/array.fn.drop.php",
      "src/fn/array.fn.first.php",
      "src/fn/array.fn.get.php",
      "src/fn/array.fn.find.php",
      "src/fn/array.fn.filter.php",
      "src/fn/array.fn.flatMap.php",
      "src/fn/array.fn.group.php",
      "src/fn/array.fn.reduce.php",
      "src/fn/array.fn.sort.php",
      "src/fn/array.fn.each.php",
      "src/Sequence/sequence.gen.php",
      "src/Pipeline/pipeline.fn.php"
    ]
  },
  "autoload-dev": {
    "psr-4": { "Arrayly\\Test\\": "tests/" }
  }
}